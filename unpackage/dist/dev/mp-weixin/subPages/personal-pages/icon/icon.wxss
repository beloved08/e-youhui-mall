.container {
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  background-color: #f6f6f6;
}
.container .button {
  margin: 0 auto;
  margin-top: 10rpx;
  display: flex;
  flex-direction: row;
}
.container .button .top-button {
  width: 520rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: row;
}
.container .button .tui-modal-custom {
  display: flex;
  flex-direction: column;
}
.container .button .tui-modal-custom .tui-modal-custom-text {
  margin: 0 auto;
  margin-top: 20rpx;
  margin-bottom: 50rpx;
}
.container .button .tui-modal-custom .tui-modal-custom-button {
  display: flex;
  flex-direction: row;
  margin: 0 auto;
}
.container .icon_preview {
  width: 95%;
  margin: 0 auto;
  margin-top: 20rpx;
  border-radius: 10rpx;
  background-color: #fff;
}
.container .icon_preview .icon_preview_item {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.container .icon_preview .icon_preview_item .icon_preview_title {
  padding: 20rpx;
  font-size: 15px;
  font-weight: bold;
}
.container .icon_preview .icon_preview_item .icon_preview_list {
  width: 95%;
}
.container .icon_preview .icon_preview_item .icon_preview_list .grid-text {
  font-size: 12px;
  color: #626468;
  padding: 10rpx 0 20rpx 0rpx;
}
.container .icon_preview .icon_preview_item .order-grid {
  text-align: center;
}
.container .icon_preview .icon_preview_item .order-grid .grid-title {
  font-size: 11px;
}
.container .icon_preview .icon_preview_item .utils-grid {
  text-align: center;
}
.container .icon_preview .icon_preview_item .utils-grid .utils-grid-title {
  font-size: 12px;
}
.container .flow {
  width: 100%;
  margin: 0 auto;
  margin-top: 40rpx;
}

